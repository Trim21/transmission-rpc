{
  "problemMatcher": [
    {
      "owner": "pylint",
      "fileLocation": [
        "relative",
        "${workspaceFolder}"
      ],
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):(\\d+):\\s+(W|E).+:\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}
